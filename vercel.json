{
    "routes": [
        { 
            "src": "/api/(.*)", 
            "dest": "https://techinsightsai-backend.vercel.app/api/$1" 
        },
        { 
            "handle": "filesystem" 
        },
        { 
            "src": "/(.*)", 
            "dest": "/index.html" 
        },
        {
            "crons": [
              {
                "path": "/api/blogs/generate",
                "schedule": "0 */3 * * *"
              }
            ]
          }
    ]
}